@model IEnumerable<string>
@Html.ActionLink("Home", "List", "Book", null, new { @class = "btn btn-block btn-degault btn-lg" })

@foreach (var link in Model)
{
    @Html.RouteLink(link, new { Controller="Book",Action="List",
                                                         specilization=link,pageno=1},
    new { @class="btn btn-block btn-default btn-lg"+
        ((link==ViewBag.SelectedSpec)?"btn-primary":"").ToString()})
}
